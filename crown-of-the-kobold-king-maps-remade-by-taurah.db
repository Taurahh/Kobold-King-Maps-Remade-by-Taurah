":{"value":"necromancy"},"components":{"focus":false,"material":false,"somatic":true,"verbal":true},"materials":{"value":""},"target":{"value":""},"range":{"value":"60 feet"},"area":{"value":10,"type":"burst","-=areaType":null},"time":{"value":"2"},"duration":{"value":"sustained up to 1 minute"},"damage":{"value":{"0":{"applyMod":false,"type":{"subtype":"","value":"slashing"},"value":"2d6"}},"applyMod":false},"save":{"value":"reflex","basic":"basic"},"sustained":{"value":false},"cost":{"value":""},"ability":{"value":""},"location":{"value":"MsxYV8aiyqR59Z1f","heightenedLevel":3},"hasCounteractCheck":{"value":false},"heightening":{"type":"interval","interval":2,"damage":{"0":"1d6"}}},"ownership":{"default":0,"FtrL3rOLD9OKBz3r":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"Q39G1KD6vMUoYVQH","name":"Vampiric Touch","type":"spell","img":"systems/pf2e/icons/spells/vampiric-touch.webp","effects":[],"folder":null,"sort":500000,"flags":{"core":{"sourceId":"Compendium.pf2e.spells-srd.N1Z1oLPdBxaSgrEE"}},"system":{"description":{"value":"<p>Your touch leeches the lifeblood out of a target to empower yourself. You deal 6d6 negative damage to the target. You gain temporary Hit Points equal to half the negative damage the target takes (after applying resistances and the like). You lose any remaining temporary Hit Points after 1 minute.</p>\n<hr />\n<p><strong>Heightened (+1)</strong> The damage increases by 2d6.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":["death","negative"],"rarity":"common","custom":""},"rules":[],"slug":"vampiric-touch","schema":{"version":0.815,"lastMigration":{"datetime":"2023-01-16T00:37:35.334-05:00","version":{"schema":0.815,"foundry":"10.291","system":"4.6.6"}}},"level":{"value":3},"spellType":{"value":"save"},"category":{"value":"spell"},"traditions":{"value":["arcane","divine","occult"]},"school":{"value":"necromancy"},"components":